<%- include('../partials/header') %>

    <table>
    <thead>
      <tr>
        <th>Airline</th>
        <th>Aiport</th>
        <th>FlightNo</th>
        <th>Departs</th>
      </tr>
    </thead>
    <tbody>
      <% flights.forEach(f => { %>
        <tr>
        <td><%= f.airline %></td>
        <td><%= f.airport %></td>
        <td><%= f.flightNo %></td>
        <td><%= f.depart %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
  
  <%- include('../partials/footer') %>